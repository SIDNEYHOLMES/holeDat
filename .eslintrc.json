{
     "parser": "@typescript-eslint/parser",
     "parserOptions": {
          "ecmaVersion": 2018,
          "sourceType": "module"
     },
     "plugins": [
          "@typescript-eslint"
     ],
     "extends": [
          "plugin:@typescript-eslint/recommended"
     ],
     "rules": {
          "@typescript-eslint/no-explicit-any": 2,
          "@typescript-eslint/no-unused-vars": 2
     }
}
